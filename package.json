{
  "name": "fourthwall-js",
  "version": "0.7.0",
  "description": "A typescript Fourthwall API client",
  "homepage": "https://github.com/MagsMagnoli/fourthwall-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/MagsMagnoli/fourthwall-js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup ./src/v0/index.ts ./src/v1/index.ts --format cjs,esm --dts --minify --clean --sourcemap",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prepublishOnly": "npm ci && npm run build",
    "start": "ts-node src/index.ts",
    "test": "jest"
  },
  "keywords": [
    "fourthwall",
    "api",
    "typescript",
    "node",
    "npm-package",
    "client",
    "http-client",
    "rest-api",
    "sdk"
  ],
  "author": "Mags Magnoli <4146037+MagsMagnoli@users.noreply.github.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/tough-cookie": "4.0.2",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsup": "8.0.1",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "axios": "1.4.0",
    "axios-cookiejar-support": "4.0.6",
    "cheerio": "1.0.0-rc.12",
    "tough-cookie": "4.1.3"
  },
  "exports": {
    "./v0": "./dist/v0/index.js",
    "./v1": "./dist/v1/index.js"
  }
}
